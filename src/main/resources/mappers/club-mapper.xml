<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="clubMapper">
	 
	
	<select id="getListCount" resultType="_int">
		select count(*)
		from tb_club
		where delete_yn ='N'
	</select>
	

</mapper>
